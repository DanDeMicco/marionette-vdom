{
  "name": "marionette-vdom",
  "version": "0.1.1",
  "description": "Marionette.js views implemented with virtual-dom",
  "main": "src/index.js",
  "scripts": {
    "start": "beefy app.js:build.js --live",
    "demo": "beefy demo/demo.js:demo/demo-build.js --live",
    "perf": "beefy perf/perf.js:perf/perf-build.js --live",
    "bundle": "browserify app.js -o build.js",
    "test": "mocha test/",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha test/ --report lcovonly -- -R spec",
    "view-cover": "istanbul report html && open ./coverage/index.html",
    "travis-test": "istanbul cover ./node_modules/mocha/bin/_mocha test/ --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "travis-saucelabs": "karma start karma.conf-ci"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiagorg/marionette-vdom"
  },
  "keywords": [
    "backbone",
    "virtual",
    "dom",
    "vdom",
    "view"
  ],
  "author": "Tiago Garcia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tiagorg/marionette-vdom/issues"
  },
  "homepage": "https://github.com/tiagorg/marionette-vdom",
  "dependencies": {
    "backbone": "^1.1.2",
    "backbone.marionette": "^2.3.0",
    "html-to-vdom": "^0.4.0",
    "jquery": "^2.1.3",
    "underscore": "^1.7.0",
    "virtual-dom": "^1.2.0"
  },
  "devDependencies": {
    "beefy": "^2.1.1",
    "browserify": "^7.0.3",
    "chai": "^1.10.0",
    "chai-jq": "0.0.8",
    "istanbul": "^0.3.5",
    "jsdom": "0.10.5",
    "karma": "^0.12.31",
    "karma-browserify": "^3.0.1",
    "karma-coverage": "^0.2.7",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-sauce-launcher": "^0.2.10",
    "mocha": "^2.1.0",
    "react": "^0.12.2",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.6.0",
    "xmldom": "^0.1.19"
  }
}
